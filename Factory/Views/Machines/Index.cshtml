@using Factory.Models

<h1> Machines </h1>

@if (Model.Count == 0)
{
  <h3><em> No machines found </em></h3>
}

@foreach (Machine machine in Model)
{
  <h5>@Html.ActionLink($"{machine.MachineName}", "Details", new { id = machine.MachineId })</h5>
}

<button>@Html.ActionLink("Add a new machine", "Create")</button>
<button>@Html.ActionLink("Home", "Index", "Home")</button>
